<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>nodeJS</title>
    <link rel="stylesheet" href="style(chat).css">
</head>
<body>

<div id="wrapper">
    <div id="loginForm">
        <h3>what is your name</h3>
        <input type="text" id="txt-username">
        <input type="button" value="Register" id="btn-register">
    </div>
    <div id="chatForm">
        <div id="left">
            <div id="box-title">
                User online
            </div>
            <div id="box-content">
            </div>
        </div>
        <div id="right">
            <div class="say-hi">
                Hello <span id="current-user">teo</span>
                <input id="btn-logout" type="button" value="logout">
            </div>
            <div class="list-msg">

                <div class="status-typing"></div>
            </div>
            <div class="wrapper-send">
                <input type="text" id="txt-Message">
                <input type="button" value="send" id="txt-Send-Message">
            </div>
        </div>
    </div>

</div>


</body>
<!--import script-->
<script src="jquery.min.js"></script>
<script src="socket.io/socket.io.js"></script>
<script src="main(chat).js"></script>

</html>